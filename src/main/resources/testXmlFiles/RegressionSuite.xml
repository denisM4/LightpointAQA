<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" verbose="1" parallel="classes" thread-count="3">

    <listeners>
        <listener class-name="by.kvach.lightBase.util.ScreenshotListener"/>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>

    <test verbose="1" name="Regression">
        <classes>
            <class name="TestCandidate"/>
            <class name="TestVacancy"/>
            <class name="TestApi"/>
        </classes>
    </test>

</suite>